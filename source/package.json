{
  "name": "streaming-data-solution",
  "version": "1.6.2",
  "description": "jsii version of stream-data-solution",
  "bin": {
    "streaming-data-solution": "bin/streaming-data-solution.js"
  },
  "main": "lib/index.ts",
  "repository": {
    "url": "https://github.com/dan-hook/streaming-data-solution-for-amazon-kinesis-and-amazon-msk.git"
  },
  "scripts": {
    "buildjsii": "jsii",
    "build": "tsc",
    "watch": "jsii -w",
    "test": "jest --coverage",
    "cdk": "cdk",
    "clean": "rm -rf node_modules && rm -rf test/__snapshots__",
    "code-linter": "./node_modules/eslint/bin/eslint.js . --ext .ts --ext .js",
    "code-formatter": "./node_modules/prettier/bin-prettier.js --config .prettierrc.yml '**/*.ts' '**/*.js' --write",
    "package": "jsii-pacmak -vvv"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.173.0",
    "@aws-cdk/assertions": "1.173.0",
    "@types/jest": "^29.1.2",
    "@types/node": "18.8.4",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "aws-cdk": "1.173.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.1.2",
    "jsii": "^1.69.0",
    "jsii-pacmak": "^1.69.0",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "~4.8.4"
  },
  "bundledDependencies": [
    "source-map-support",
    "vm2"
  ],
  "dependencies": {
    "@aws-cdk/aws-cloud9": "1.173.0",
    "@aws-cdk/aws-cloudwatch": "1.173.0",
    "@aws-cdk/aws-cognito": "1.173.0",
    "@aws-cdk/aws-ec2": "1.173.0",
    "@aws-cdk/aws-glue": "1.173.0",
    "@aws-cdk/aws-kinesis": "1.173.0",
    "@aws-cdk/aws-kinesisanalytics": "1.173.0",
    "@aws-cdk/aws-kinesisfirehose": "1.173.0",
    "@aws-cdk/aws-kms": "1.173.0",
    "@aws-cdk/aws-lambda": "1.173.0",
    "@aws-cdk/aws-logs": "1.173.0",
    "@aws-cdk/aws-msk": "1.173.0",
    "@aws-cdk/aws-s3": "1.173.0",
    "@aws-cdk/cloudformation-include": "1.173.0",
    "@aws-cdk/core": "1.173.0",
    "@aws-solutions-constructs/aws-apigateway-kinesisstreams": "1.173.0",
    "@aws-solutions-constructs/aws-apigateway-lambda": "1.173.0",
    "@aws-solutions-constructs/aws-kinesisfirehose-s3": "1.173.0",
    "@aws-solutions-constructs/aws-kinesisstreams-lambda": "1.173.0",
    "source-map-support": "^0.5.21",
    "vm2": "^3.9.11"
  },
  "private": true,
  "license": "Apache-2.0",
  "author": {
    "name": "aws-solutions-builder"
  },
  "stability": "stable",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "versionFormat": "full",
    "targets": {
      "python": {
        "distName": "cdk-streaming",
        "module": "cdk_streaming"
      }
    }
  }
}